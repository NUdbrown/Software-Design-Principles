public class Controller {
	private Model model;
	
	public Controller(Model model){
		this.model = model;
	}
		
	//cases: if char , backspace, left or right cursor
	public void sendCharsInt(int keyPress) {
		int backspace = 8;
		int leftArrow = 17;
		int rightArrow = 19;
		
		switch(keyPress){
		case 8:		
			 break;
		case 17:
			break;
		case 19:
			break;
		default:
			break;
		}
		
		
	}

}
